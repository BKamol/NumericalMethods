    "#kij - algorithm LU\n",
    "def kij_LU(A):\n",
    "    n = len(A)\n",
    "    for k in range(1, n):\n",
    "        for i in range(k+1, n):\n",
    "            A[i][k] = A[i][k]/A[k][k]\n",
    "            for j in range(k+1, n):\n",
    "                A[i][j] = A[i][j] - A[i][k]*A[k][j]\n"
   ]
